--Glider Needed Resets everytime you DIE (this script only works if your a nigger)

game:GetService("Workspace").Gliders:Destroy()

while true do wait(0.1)
local args = {
    [1] = {}
}

game:GetService("ReplicatedStorage").rbxts_include.node_modules:FindFirstChild("@rbxts").net.out._NetManaged.HangGliderUse:FireServer(unpack(args))

end
